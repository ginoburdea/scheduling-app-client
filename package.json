{
    "name": "scheduling-app-client",
    "private": true,
    "version": "0.5.0",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc && vite build",
        "preview": "vite preview",
        "cy:open": "cypress open",
        "cy:run": "cypress run --browser chrome --quiet",
        "test": "concurrently --kill-others --prefix-colors green,yellow --names app,cypress npm:dev npm:cy:open",
        "test:ci": "concurrently --success command-cypress --kill-others --prefix-colors green,yellow --names app,cypress npm:dev npm:cy:run",
        "lint": "eslint --ignore-path .gitignore --fix **/*!(.{c,d}).{js,ts,vue}",
        "format": "prettier --write .",
        "prepare": "husky install"
    },
    "dependencies": {
        "@vueuse/core": "^9.11.1",
        "axios": "^1.2.3",
        "crypto-js": "^4.1.1",
        "dayjs": "^1.11.7",
        "pinia": "^2.0.29",
        "pinia-plugin-persistedstate": "^3.0.2",
        "sentence-case": "^3.0.4",
        "uuid": "^9.0.0",
        "vue": "^3.2.45",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.6.0",
        "@tailwindcss/forms": "^0.5.3",
        "@types/chai-string": "^1.4.2",
        "@types/crypto-js": "^4.1.1",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.11.18",
        "@types/uuid": "^9.0.0",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "@vitejs/plugin-vue": "^4.0.0",
        "@volar/vue-typescript": "^1.0.24",
        "autoprefixer": "^10.4.13",
        "chai-string": "^1.5.0",
        "concurrently": "^7.6.0",
        "cypress": "^12.3.0",
        "dotenv": "^16.0.3",
        "eslint": "^8.32.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-vue": "^9.9.0",
        "husky": "^8.0.3",
        "lint-staged": "^13.1.0",
        "openapi-typescript": "^6.1.0",
        "openapi-typescript-fetch": "^1.1.3",
        "postcss": "^8.4.21",
        "prettier": "^2.8.3",
        "prettier-plugin-organize-imports": "^3.2.2",
        "prettier-plugin-tailwindcss": "^0.2.1",
        "tailwindcss": "^3.2.4",
        "typescript": "^4.9.3",
        "vite": "^4.0.0",
        "vite-plugin-eslint": "^1.8.1",
        "vue-eslint-parser": "^9.1.0",
        "vue-tsc": "^1.0.11"
    }
}
